{
  "name": "@fispo/template-theme",
  "version": "0.0.0",
  "description": "themes of fispo",
  "main": "./src/index.ts",
  "types": "./src/types/index.ts",
  "type": "module",
  "scripts": {
    "dev": "fispo dev",
    "docs:build": "fispo build",
    "preview": "fispo preview",
    "start": "tsup --watch --format=cjs,esm",
    "build": "tsup --format=cjs,esm"
  },
  "exports": {
    ".": {
      "types": "./src/types/index.ts",
      "import": "./src/index.ts"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json",
    "./config": "./src/config.ts"
  },
  "keywords": [
    "react",
    "theme",
    "fispo"
  ],
  "author": "XXX17",
  "license": "MIT",
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "^1.85.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "fispo-core": "^0.4.0"
  },
  "files": [
    "src",
    "package.json",
    "tsconfig.json"
  ]
}
