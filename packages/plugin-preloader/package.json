{
  "name": "@fispo/plugin-preloader",
  "version": "0.4.4",
  "description": "loading animation of blog",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "tsup --watch --format=cjs,esm",
    "build": "tsup --format=cjs,esm"
  },
  "keywords": [
    "plugin",
    "animation",
    "fispo"
  ],
  "author": "XXX17",
  "license": "ISC"
}
